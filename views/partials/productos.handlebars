{{#if listaProductos}}
<div class="table-responsive">
    <table class="table table-dark">
        <tr style="color: yellow;">
            <th>Name</th>
            <th>Title</th>
            <th>Image</th>
        </tr>
        {{#each productos}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td><img src={{this.thumbnail}} /></td>
        </tr>
        {{/each}}
    </table>
</div>
{{else}}
<h3 class="alert alert-danger">no se encontraron datos</h3>

{{/if}}


